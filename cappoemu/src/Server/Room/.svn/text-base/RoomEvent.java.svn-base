package Server.Room;

import java.util.List;

/*
 *****************
 * @author capos *
 *****************
*/

public class RoomEvent
{
    public int RoomId;
    public String Name;
    public String Description;
    public int Category;
    public List<String> Tags;
    public String StartTime;


    public RoomEvent(int roomId, String name, String description, int category, List<String> tags, int time)
    {
        RoomId = roomId;
        Name = name;
        Description = description;
        Category = category;
        Tags = tags;
        StartTime = Integer.toString(time);
    }
}
